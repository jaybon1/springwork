<mapper namespace="com.jaybon.springStudy.repository.UserRepository">

	<select id="findById" resultType="com.jaybob.springStudy.model.User">
		select * from user where id = #{id}
	</select>
	
	<select id="findAll" resultType="com.jaybob.springStudy.model.User">
		select * from user
	</select>

