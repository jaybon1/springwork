package com.jaybon.mySecurityTest.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class IndexController {

	@GetMapping({"","/"})
	public String index() {
		return "인덱스 페이지입니다";
	}
	
	@GetMapping("/user")
	public String user() {
		return "인덱스 페이지입니다";
	}
	
	@GetMapping("/admin")
	public String admin() {
		return "인덱스 페이지입니다";
	}
	
	@GetMapping("/login")
	public String login() {
		return "로그인 페이지입니다";
	}
	
	@GetMapping("/join")
	public String join() {
		return "회원가입 페이지입니다";
	}
	
}
